{
  "env": { "browser": true, "es2022": true },
  "extends": ["eslint:recommended"],
  "parserOptions": { "ecmaVersion": "latest", "sourceType": "module" },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "complexity": ["warn", 12]
  }
}
